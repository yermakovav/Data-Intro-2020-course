void setup() {
  size(500, 400);
  background(10, 80, 100);
}

void draw() {
  if (mousePressed) {
    background(10, 80, 100);
  } 
stroke (0,0,150);
fill(248,165,247);
ellipse(mouseY, mouseX, 300, 300);
stroke(255);
fill(88,152,77);
rect(245, 40, 10, 240);
stroke (0);
fill (0);
ellipse(190, 200, 70, 70);
ellipse (310,200,70,70);
stroke(255);
fill (255);
ellipse (mouseX,mouseY,20,20);
rect (150,100,200,10)
  ellipse(mouseX, mouseY, 70, 70);
}
